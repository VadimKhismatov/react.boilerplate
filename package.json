{
  "name": "react.boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --env development --hot --history-api-fallback",
    "start:open": "webpack-dev-server --mode development --env development --open --hot --history-api-fallback",
    "build:dev": "rimraf dist && webpack --mode development --env development",
    "build:dev:watch": "rimraf dist && webpack --watch --mode development --env development",
    "build:prod": "rimraf dist && webpack --mode production  --env production ",
    "build:prod:watch": "rimraf dist && webpack --watch --mode production  --env production ",
    "server": "node server"
  },
  "author": "Vadim Khismatov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.46",
    "@babel/preset-env": "7.0.0-beta.46",
    "@babel/preset-react": "7.0.0-beta.46",
    "@babel/preset-stage-2": "7.0.0-beta.46",
    "babel-loader": "~8.0.0-beta.2",
    "css-loader": "~0.28.11",
    "extract-text-webpack-plugin": "~4.0.0-beta.0",
    "html-webpack-plugin": "~3.2.0",
    "node-sass": "~4.8.3",
    "optimize-css-assets-webpack-plugin": "~4.0.0",
    "sass-loader": "~7.0.1",
    "style-loader": "~0.21.0",
    "uglifyjs-webpack-plugin": "~1.2.5",
    "webpack": "~4.16.0",
    "webpack-cli": "~2.0.15",
    "webpack-dev-server": "~3.1.3",
    "webpack-merge": "~4.1.3",
    "react": "~16.4.1",
    "react-dom": "~16.4.1",
    "react-router": "~4.3.1",
    "react-router-dom": "~4.3.1",
    "express": "~4.16.3",
    "postcss-loader": "~2.1.5",
    "autoprefixer": "~8.6.4",
    "file-loader": "~1.1.11",
    "image-webpack-loader": "3.2.0"
  }
}
